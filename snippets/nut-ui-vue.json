  "Layout: Grid": {
    "prefix": "ngrid",
    "body": [
      "<nut-grid :column-num=\"$1\" direction=\"${2|vertical|horizontal|}\">",
      "\t<nut-grid-item v-for=\"(${3:grid}, ${4:index}) in ${5:grids}",
      "\t:key=\"${6:index}\"",
      "\ttext=\"$7\"",
      "\t\\>",
      "\t</nut-grid-item>",
      "</nut-grid>",
      "$8"
    ],
    "description": "Nut UI <nut-grid>"
  },
  "Layout: Layout": {
