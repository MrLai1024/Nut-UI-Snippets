{
  "name": "nut-ui-snippets",
  "displayName": "Nut UI Snippets",
  "description": "Nut UI组件标签快捷键,支持Vue和React",
  "version": "0.0.1",
  "publisher": "Jason Lai",
  "author": {
    "name": "Jason Lai"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "activationEvents": [
    "onLanguage:vuejs"
  ],
  "main": "./out/extension",
  "categories": [
    "Snippets"
  ],
  "license": "MIT",
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/nut-ui-vue.json"
      },
      {
        "language": "html",
        "path": "./snippets/nut-ui-vue.json"
      },
      {
        "language": "vue",
        "path": "./snippets/nut-ui-vue.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/nut-ui-vue.json"
      }
    ]
  }
}
